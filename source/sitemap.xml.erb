<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <% page_map = sitemap.resources.keep_if { |resource| resource.content_type.eql?("text/html; charset=utf-8") } %>
  <% page_map.each do |resource| %>
  <url>
    <loc><%= data.site.url %><%= resource.url %></loc>
    <priority>0.5</priority>
  </url>
  <% end %>
</urlset>
